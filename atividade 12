package primeiro;
import java.util.Scanner;
public class Teste {

	public static void main(String[] args) {
		
			Scanner scanner = new Scanner(System.in);
			System.out.println("Imprimindo numeros impares entre 1 e 100");
			for (int i=2; i <= 100; i++) {
				if (i % 2 != 0) {
					 System.out.println(i);
					 
				}
			}
	
		}
}
	
